package com.sgm.dms.am.client;



/**
 * 单点登录用户安全证书
 * 
 * @author LEIKZHU
 *
 */
public interface SSOCertificate {

	/**
	 * 取得登录用户编码
	 * 
	 * @Author : LEIKZHU
	 * @Date : Aug 31, 2014
	 * @return 登录用户编码
	 */
	public String getUserId();

	/**
	 * 当前登录用户与上一次相比，是否改变
	 * 
	 * @Author : LEIKZHU
	 * @Date : Aug 31, 2014
	 * @return "true"发生改变,"false"未改变
	 */
	public boolean isChange();
}
